<%- include('../partials/admin/header') %>
<%- include('../partials/admin/sidePanel') %>

<div class="main-panel">
  <div class="content-wrapper">
    <a href="/admin/products/add-product" class="btn btn-primary mb-3"
      >Add Product</a
    >
    <div class="row">
      <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            <h4 class="card-title">Product Management</h4>
            <div class="table-responsive">
              <table class="table table-striped">
                <thead>
                  <tr>
                    <th>No</th>
                    <th>Product Name</th>
                    <th>Images</th>
                    <th>Category</th>
                    <th>Quantity</th>
                    <th>Price</th>
                    <th>Offer in Percentage</th>
                    <th>Offer Price</th>
                    <th>Actions</th>
                  </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</div>
